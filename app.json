{
  "expo": {
    "name": "learneng",
    "slug": "learneng",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "permissions": [
          "WAKE_LOCK"
        ]
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cr7cr8.learneng"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9c11f8aa-c153-480c-bc04-d1f72fef5f97"
      }
    },
    "plugins": [
      "expo-background-task"
    ]
  }
}